{
  "name": "battleship",
  "version": "1.0.0",
  "description": "Battleship game on the browser",
  "private": true,
  "scripts": {
    "build": "npx webpack --config webpack.prod.js",
    "start": "npx webpack serve --no-open --config webpack.dev.js",
    "test": "jest test/tests/",
    "test:build": "npx babel src --out-dir test --ignore src/index.js",
    "test:build:watch": "npx babel -w src --out-dir test --ignore src/index.js"
  },
